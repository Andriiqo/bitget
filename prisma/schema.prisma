generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = "file:./dev.db?connection_limit=1&socket_timeout=60000"
}

model Ticker {
  id        Int    @id @default(autoincrement()) // Идентификатор пользователя, автоинкремент
  symbol    String @unique
  exchange  String
  price     String
  timestamp String
}
